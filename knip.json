{
  "$schema": "https://unpkg.com/knip@latest/schema.json",
  "entry": [
    "app/**/*.{js,ts,jsx,tsx}",
    "app/**/page.{js,ts,jsx,tsx}",
    "app/**/layout.{js,ts,jsx,tsx}",
    "app/**/loading.{js,ts,jsx,tsx}",
    "app/**/error.{js,ts,jsx,tsx}",
    "app/**/not-found.{js,ts,jsx,tsx}",
    "app/**/route.{js,ts,jsx,tsx}",
    "app/**/template.{js,ts,jsx,tsx}",
    "app/**/default.{js,ts,jsx,tsx}",
    "middleware.{js,ts}",
    "next.config.{js,ts}",
    "instrumentation.{js,ts}"
  ],
  "project": [
    "app/**/*.{js,ts,jsx,tsx}",
    "components/**/*.{js,ts,jsx,tsx}",
    "lib/**/*.{js,ts,jsx,tsx}",
    "hooks/**/*.{js,ts,jsx,tsx}"
  ],
  "ignore": [
    "**/*.config.{js,ts}",
    "**/node_modules/**",
    ".next/**",
    "public/**"
  ],
  "ignoreDependencies": [
    "@types/node",
    "@types/react",
    "@types/react-dom",
    "eslint",
    "eslint-config-next",
    "typescript"
  ]
}
